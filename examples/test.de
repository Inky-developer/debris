fn is_prime(n: Int) -> Bool {
    let x = dyn_int(n / 2);
    loop {
        if x <= 1 {
            return false;
        }

        if n % x == 0 {
            return true;
        }

        x = x - 1;
    }
}

let value = dyn_int(1);
loop {
    print(value);
    print(dyn_int(is_prime(value)));
    value = value + 1;
}
