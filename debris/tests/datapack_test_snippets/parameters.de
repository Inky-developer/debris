# passed by value
fn add_one(x: Int) {
    x = x + 1;
}

let a = 1;
print(a);
add_one(a);
print(a);

struct Foo {
    a: Int

    fn mutate(self: Foo) {
        self.a = self.a + 1;
    }
}


let foo = Foo {a: 5};
print(`$foo`);
foo.mutate();
let is_six = foo.a == 6;
add_one(foo.a);
let is_still_six = foo.a == 6;

a == 1 and is_six and is_still_six