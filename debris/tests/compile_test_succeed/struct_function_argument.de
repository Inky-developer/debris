struct Foo {
    a: Int
}

fn do_stuff_with_type(strukt: Struct) {
    print(`$strukt`);
}

fn do_stuff_with_foo(foo: Foo) {
    print(foo.a + 2);
}

fn default_foo() -> Foo {
    let value = Foo {a: 0};
    value
}

let fodo = default_foo();
fodo.a = 1209;

do_stuff_with_type(Foo);
do_stuff_with_foo(fodo);